<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	<bean class="com.jspiders.springcore.bean.PersonBean"
		name="person1">
		<property name="id" value="1"/>
		<property name="name" value="aditya"/>
		<property name="email" value="aditya@email.com"/>
		<property name="contact" value="99425525243"/>
		<property name="aadhar" ref="aadhar1"/>
	</bean>
	
	
	<bean class="com.jspiders.springcore.bean.AadharBean" name="aadhar1">
	<property name="id" value="1"/>
	<property name="aadhar_no" value="12345678"/>
	<property name="date_of_issue" value="15-Jun-2015"/>
	</bean>
</beans>